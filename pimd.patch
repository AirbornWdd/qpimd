+      vty_out (vty, "%-16r %-10s%-8d v2/S   %-6d %-5d ",
+	       &(vif->addr), vif->ifp->name, vif->index, 
+	       vif->neighbor_cnt, vif->hello.priority);
+      vty_out (vty, "%r",
+	       CHECK_FLAG (vif->flags, PIM_VIF_FLAG_DR) 
+	       ? &(vif->addr) : &(vif->dr_addr));
