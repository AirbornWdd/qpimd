+              vty_out (vty, "   %r\n", addr);
+            }
+        }
+      vty_out (vty, "  Neighbors:\n");
+      /* Neighbors */
+      for (rn = route_top (vif->neighbor); rn; rn = route_next (rn))
+        {
+          if ((nbr = rn->info) == NULL)
+            continue;
+          vty_out (vty, "   %r\n", &nbr->addr);
+        }
